  <div className=\"content\">\n            <Routes>\n              <Route exact path='/' element={<Home />} />\n              <Route exact path='/create' element={<Create/>} />\n              <Route exact path='/notes/:id' element={<NoteDetails />}/>\n              <Route exact path='/Login' element={<Login />}/>\n              <Route exact path='/Signup' element={<Signup />}/>\n              <Route exact path=\"/forgot-Password\" element={<ForgotPassword />} />\n              <Route path='*' element={<NotFound />}/>\n            </Routes>\n          </div>\n\n        </AuthProvider>\n        </div>\n      </Router>\n  );\n}\n\nexport default App;\n","/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/Navbar.js",["90","91"],[],"import { Link, useNavigate } from \"react-router-dom\";\nimport { useAuth } from \"../contexts/AuthContext\";\nimport { Button } from \"react-bootstrap\";\nimport { useState } from \"react\";\n\n\nconst Navbar = () => {\n  const {currentUser, logout} = useAuth()\n  const [error, setError] = useState('')\n  const navigate = useNavigate()\n\n  const handleLogout = async() => {\n    setError('')\n    try{\n      await logout()\n      navigate('/Login')\n    }catch{\n      setError('Failed to Logout')\n    }\n  }\n\n  return ( \n    <nav className=\"navbar\">\n      <Link to='/'>\n      <h1>NOTELY</h1>\n      </Link>\n      <div className=\"links\">\n        <Link to='/'>HOME</Link>\n        <Link className=\"new_btn\" to='/create'>NEW NOTE</Link>\n        {currentUser ? <Link onClick={handleLogout} className=\"new_btn mt-3\" to='/Login'>Log Out</Link> : <Link className=\"new_btn\" to='/Login'>Login</Link>}\n      </div>\n    </nav>\n  );\n}\n \nexport default Navbar\n","/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/Create.js",["92"],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/NoteDetails.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/Home.js",["93"],[],"import { Alert } from \"react-bootstrap\";\nimport { useAuth } from \"../contexts/AuthContext\";\nimport useFetch from \"../useFetch\";\nimport NoteList from \"./NoteList\";\n\nconst Home = () => {\n  const { error, isPending, notes: notes } = useFetch()\n  const {currentUser} = useAuth()\n  return (\n    <div className=\"home\">\n      {!currentUser && <Alert className=\"text-center\" variant='primary'>SIGN UP / LOGIN</Alert>}\n      { error && <div> {error} </div>}\n      { currentUser && isPending && <div> loading ...</div>}\n      { notes && <NoteList notes= {notes} />}\n    </div>\n  );\n}\n \nexport default Home;","/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/NoteList.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/contexts/AuthContext.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/Login.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/Signup.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/firebase.js",[],[],"/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/forgotPassword.js",["94","95","96"],[],"import { useState, useRef } from \"react\";\nimport { Card, Button, Form } from \"react-bootstrap\";\nimport { useAuth } from \"../contexts/AuthContext\";\nimport { Link } from \"react-router-dom\";\n\nconst ForgotPassword = () => {\n  const emailRef = useRef()\n  const {passwordReset} = useAuth()\n  const [error, setError] = useState('')\n  const [message, setMessage] = useState('')\n  const [loading, setLoading] = useState(false)\n\n  const handleSubmit =  async(e) => {\n    e.preventDefault()\n    try{\n      setMessage('')\n      setLoading(true)\n      setError('')\n      await passwordReset(emailRef.current.value)\n      setMessage('Check your Inbox to reset')\n    }catch{\n      setError('Reset Email Not Sent')\n    }\n  }\n\n  return (\n    <div className=\"forgotPassword\">\n      <Card>\n        <Card.Body>\n          <h2 className=\"text-center\">Password Reset</h2>\n          <Form>\n            <Form.Group>\n              <Form.Label>Email</Form.Label>\n              <Form.Control type='email' ref={emailRef} required></Form.Control>\n            </Form.Group>\n            <Button className=\"w-100 mt-2\" type=\"submit\" disabled={loading}>Reset</Button>\n          </Form>\n          <div className=\"loginLink text-center\">\n            <Link to='/Login'>Log in</Link>\n          </div>\n        </Card.Body>\n      </Card>\n    </div>\n  );\n}\n \nexport default ForgotPassword;","/Users/mc/Desktop/The_Odin_Project/react/notely/src/Components/CreateCollection.js",["97","98"],[],"import { collection, addDoc, doc, setDoc } from 'firebase/firestore';\nimport { firestore, auth } from '../firebase';\n\nconst createCollectionForUser = async () => {\n  try {\n    const user = auth.currentUser;\n    const userId = user.uid;\n    const collectionName = `UserNotes_${userId}`; // Create a unique collection name for each user\n\n    const newCollectionRef = collection(firestore, collectionName);\n    const newDocRef = doc(collection(firestore, collectionName));\n    \n    await setDoc(newDocRef, {\n      id: newDocRef.id, // Document ID as a field\n      title: 'WELCOME NOTE'\n    });\n\n    console.log('Collection created for user:', userId);\n  } catch (error) {\n    console.error('Error creating collection for user:', error);\n  }\n};\n\nexport default createCollectionForUser;",{"ruleId":"99","severity":1,"message":"100","line":3,"column":50,"nodeType":"101","messageId":"102","endLine":3,"endColumn":58},{"ruleId":"99","severity":1,"message":"103","line":3,"column":60,"nodeType":"101","messageId":"102","endLine":3,"endColumn":71},{"ruleId":"99","severity":1,"message":"104","line":3,"column":10,"nodeType":"101","messageId":"102","endLine":3,"endColumn":16},{"ruleId":"99","severity":1,"message":"105","line":9,"column":10,"nodeType":"101","messageId":"102","endLine":9,"endColumn":15},{"ruleId":"99","severity":1,"message":"106","line